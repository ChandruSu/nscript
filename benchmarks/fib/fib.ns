
let std = import("std");
let N = std.parseInt(args[0]);
let t0 = std.time();

fun fib(n) {
  if n == 1 {
    return 0;
  } else if n < 4 {
    return 1;
  } else {
    return fib(n - 1) + fib(n - 2);
  }
}

std.println(fib(N));

let t1 = std.time();

std.println("Execution took (ms) " + std.str(t1 - t0));