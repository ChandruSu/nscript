
let std = import("std");

let N = 1000;
let iterations = 1000;

std.println("Performing GC stress test");

let i = 0;
while i < iterations {
  std.println("Iteration: " + std.str(i));

  let arr = [];

  let j = 0;
  while j < N {
    std.append(arr, j);
    j += 1;
  }

  i += 1;
}

std.println("GC stress test complete.");
