

let std = import("std");

fun hi() {
    return {
        "a": 3
    };
}

fun mul(x) {
    return fun(y) {
        return x * y;
    };
}

# object will be collected
std.println(hi().a);

# object will not be collected
let x = hi();
x.a = 4;

std.println(x.a);

std.println(mul(2)(5));

std.gc();
