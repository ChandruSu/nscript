
fun is_prime(n) {
    let i = 2;
    while i < n {
        if n % i == 0 {
            return false;
        }
        i += 1;
    }
    return true;
}

fun find_prime(n) {
    let p = 2;
    let i = 2;
    while n > 0 {
        if is_prime(i) {
            p = i;
            n -= 1;
        }
        i += 1;
    }
    return p;
}

let prime_10 = find_prime(1000);    